prototype(Neos.NeosIo:ImageTeaser) < prototype(Neos.Neos:Content) {
    link.@process.convertUris = Neos.Neos:ConvertUris {
        absolute = true
    }

    title = null
    // is an asset. Either image or imageUrl must be specified.
    image = null
    // is an URL. Either image or imageUrl must be specified.
    imageUrl = null
    text = null
    className = null

    sectionName = 'main'
}
