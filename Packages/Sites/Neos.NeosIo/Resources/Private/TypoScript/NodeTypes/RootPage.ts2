/**
 * DocumentNodeTypes around the Root
 */

prototype(Neos.NeosIo:RootPage.Document) < prototype(Neos.NeosIo:DefaultPage) {
	body {
		templatePath = 'resource://Neos.NeosIo/Private/Templates/Page/RootPage.html'

		content {
			keyVisual = PrimaryContent {
				nodePath = 'keyVisual'
			}

			main = PrimaryContent {
				nodePath = 'main'
			}
		}
	}
}
